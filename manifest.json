{
  "manifest_version": 3,
  "name": "MuleBuy Popup Remover",
  "version": "1.1",
  "description": "Removes the 'keywords-modal' div from mulebuy.com product pages when a button is clicked. Have fun shopping!",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "MuleBuy Popup Remover"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },  
  "host_permissions": [
    "https://mulebuy.com/*"
  ]
}
